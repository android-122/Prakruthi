    <androidx.appcompat.widget.AppCompatButton
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:id="@+id/wishlist_login_btn"
        android:layout_below="@id/textView18"
        android:textAlignment="center"
        android:text="Add to Cart"
        android:textAllCaps="false"
        android:fontFamily="@font/ibarra_real_nova"
        android:textSize="@dimen/_20sdp"
        android:background="@drawable/custom_shape_btn"
        android:textStyle="bold"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="@dimen/_100sdp"
        android:layout_marginEnd="@dimen/_10sdp"
        android:layout_marginTop="@dimen/_100sdp"

        />


    <androidx.cardview.widget.CardView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:cardCornerRadius="5dp"
        app:cardElevation="0dp"
        app:layout_constraintEnd_toEndOf="@+id/wishlist_login_btn"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/imageView5">

        <TextView
            android:id="@+id/item3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="Plastic Glass, Copper amp, Wood Meterial "> </TextView>


        <TextView
            android:id="@+id/add_date_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="Added on April 3, 2023 "

            android:layout_marginRight="@+id/item3"

            > </TextView>


    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>